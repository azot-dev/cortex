{
  "manifest_version": 3,
  "name": "Cortex Devtools",
  "version": "1.0",
  "description": "Debugger for the npm package @azot-dev/cortex",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost/*",
      "https://localhost/*"
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html"
}